
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body ng-app="app">
    <div ng-controller="democontroller"> 
        <h1>{{title}}</h1>
        <hr />
        <input type="button" value="GetAll" ng-click="getAll()" />
        <input type="button" value="GetOne" ng-click="getOne()" />
        <input type="button" value="Save" ng-click="save()" />
        <input type="button" value="Delete" ng-click="delete()" />
        <input type="button" value="Update" ng-click="update()" />
    </div>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-resource.js"></script>
    <script src="~/Scripts/app/app.js"></script>
    <script src="~/Scripts/app/Services/apiservice.js"></script>
    <script src="~/Scripts/app/Controllers/democontroller.js"></script>
    
</body>
</html>
